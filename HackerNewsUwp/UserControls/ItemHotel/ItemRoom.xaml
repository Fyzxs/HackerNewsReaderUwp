<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:userControls="using:HackerNewsUwp.UserControls"
    x:Class="HackerNewsUwp.UserControls.ItemHotel.ItemRoom">

    <Grid Loaded="Grid_Loaded">

        <userControls:QgSetTextBox x:Name="TxtTitle" Margin="0,0,10,0" TextWrapping="NoWrap" Text="Title Box" HorizontalAlignment="Left" VerticalAlignment="Top" IsEnabled="False"/>
        <userControls:QgSetTextBox x:Name="TxtAuthor" Margin="0,32,10,10" TextWrapping="NoWrap" Text="Author Box" HorizontalAlignment="Left" VerticalAlignment="Top" IsEnabled="False"/>

    </Grid>
</UserControl>
