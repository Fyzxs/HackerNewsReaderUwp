<Page xmlns:itemListHotel="using:HackerNewsUwp.UserControls.ItemListHotel" 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:userControls="using:HackerNewsUwp.UserControls"
    x:Class="HackerNewsUwp.Screens.MainPageHotel.MainPageView"
    mc:Ignorable="d" Loaded="Page_Loaded">
    <Canvas HorizontalAlignment="Left" VerticalAlignment="Top">
        <userControls:QgSetTextBox x:Name="TxtTitle" Width="232" Height="25" Text="[Story TitleInto]" VerticalAlignment="Top" Canvas.Top="1" Canvas.Left="1" HorizontalAlignment="Left" HorizontalContentAlignment="Left" IsReadOnly="True" PlaceholderText="[Story TitleInto]"/>

        <itemListHotel:ItemListRoom x:Name="LstItems" Width="232" Height="420" VerticalAlignment="Top" Canvas.Top="50" Canvas.Left="1" HorizontalAlignment="Left" HorizontalContentAlignment="Left"/>
    </Canvas>


</Page>

